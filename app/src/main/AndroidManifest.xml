<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lyushiwang.netobserve">

    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">

        <activity android:name="com.lyushiwang.netobserve.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.lyushiwang.netobserve.project_manage" />
        <activity android:name="com.lyushiwang.netobserve.create_new_project" />
        <activity android:name="com.lyushiwang.netobserve.system_setting" />
        <activity android:name="com.lyushiwang.netobserve.setting_station" />
        <activity android:name="com.lyushiwang.netobserve.setting_tolerance_horizontal"/>
        <activity android:name="com.lyushiwang.netobserve.setting_tolerance_vertical"/>
        <activity android:name="com.lyushiwang.netobserve.setting_tolerance_distance"/>
        <activity android:name="com.lyushiwang.netobserve.setting_weather"/>
        <activity android:name="com.lyushiwang.netobserve.setting_common"/>
        <activity android:name="com.lyushiwang.netobserve.observe_manage"/>
        <activity android:name="com.lyushiwang.netobserve.observe_now"/>
        <activity android:name="com.lyushiwang.netobserve.observe_known_point"/>
    </application>

</manifest>